<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oreo.mcommonjobs">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignInActivity" />
        <activity android:name=".Activity.ProfileSetUpActivity" />
        <activity android:name=".Activity.SelectUserTypeActivity" />
        <activity android:name=".Activity.NavigationActivityForJobProvider" />
        <activity android:name=".Activity.CreateJobPostActivity" />
        <activity android:name=".Activity.NavigationActivityForJobSeeker" />
        <activity android:name=".Activity.ViewJobsActivity" />
        <activity android:name=".Activity.JobInfoActivity" />
        <activity android:name=".Activity.ViewApplicants" />
        <activity android:name=".Activity.AddProfileActivity" />
        <activity android:name=".Activity.ProviderRatingMenuActivity" />
        <activity android:name=".Activity.ProviderRatingsListActivity" />
        <activity android:name=".Activity.ProviderJobsListActivity" />
        <activity android:name=".Activity.ProviderEmployeesListActivity" />
        <activity android:name=".Activity.ProviderViewRatingActivity" />
        <activity android:name=".Activity.ProviderRateActivity" />
        <activity android:name=".Activity.ViewProfilesActivity" />
        <activity android:name=".Activity.ApplicationQuestionsActivity" />
        <activity android:name=".Activity.ViewApplicantCredentials" />
        <activity android:name=".Activity.ViewPendingApplicationsActivity" />

        <activity android:name=".Activity.AccountActivity" />
        <activity android:name=".Activity.UploadImageActivity" />

        <activity android:name=".Activity.SeekerRatingMenuActivity" />
        <activity
            android:name=".Activity.SeekerRatingsListActivity"
            android:label="@string/title_activity_seeker_ratings_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.SeekerEmployersListActivity" />
        <activity android:name=".Activity.SeekerViewRatingActivity" />
        <activity android:name=".Activity.SeekerRateActivity"></activity>

    </application>

</manifest>