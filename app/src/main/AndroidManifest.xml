<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oreo.mcommonjobs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".Activtity.MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activtity.UserNameSetupActivity" />
        <activity android:name=".Activtity.UserProfileSetupActivity" />
        <activity android:name=".Activtity.ProfileMenuActivity" />
        <activity android:name=".Activtity.UserTypeSelectionActivity" />
        <activity android:name=".Activtity.SignInActivity" />
        <activity android:name=".Activtity.UserPictureSetupActivity" />
        <activity android:name=".Activtity.JobSeekerNavigationMenuActivity" />
        <activity android:name=".Activtity.JobProviderNavigationMenuActivity" />
        <activity android:name=".Activtity.ProfileSkillSelectionActivity"></activity>
    </application>

</manifest>