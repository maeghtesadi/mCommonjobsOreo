<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.oreo.mcommonjobs.Activtity.SeekerRatingsListActivity">

    <TextView
        android:id="@+id/SeekerRatingsTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:fontFamily="Asap-Regular"
        android:text="@string/my_ratings_seeker"
        android:textAlignment="center"
        android:textColor="#06bebd"
        android:textSize="44sp" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/seekerRatingslist"
        android:layout_below="@+id/SeekerRatingsTitle">
    </ListView>

</RelativeLayout>
